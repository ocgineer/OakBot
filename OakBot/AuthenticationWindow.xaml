<Window x:Class="OakBot.AuthenticationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:view="clr-namespace:OakBot.View"
        mc:Ignorable="d"
        Title="Authentication" Height="480" Width="400" WindowStyle="ToolWindow" ResizeMode="NoResize" ShowInTaskbar="True">
    <Grid Background="White">
        <WebBrowser x:Name="AuthWebBrowser" Navigated="AuthWebBrowser_Navigated" LoadCompleted="AuthWebBrowser_LoadCompleted" Navigating="AuthWebBrowser_Navigating" />
    </Grid>
</Window>
